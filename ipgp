usage() unless $ARGV[0];

my $ip = $ARGV[0];

usage() unless $ip =~ /\A\d+\.\d+\.\d+\.\d+\z/;

my @ip_parts = map { usage() if $_ > 255; sprintf("%02x", $_) } split(/\./, $ip);

for(my $i=0;$i<@ip_parts;$i++) {
    printf(".") if($i);
    printf("%s", $words->{ $ip_parts[$i] }->[ $i % 2 ]);
}
printf("\n");
